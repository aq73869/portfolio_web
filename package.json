    "react-helmet-async": "^2.0.4",
    "react-lazy-load-image-component": "^1.6.0",
    "react-intersection-observer": "^9.5.3"
  },
  "devDependencies": {
    "@eslint/js": "^8.55.0",
    "@types/react": "^18.2.42",
